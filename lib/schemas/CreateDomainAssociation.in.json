{
    "type": "object",
    "properties": {
        "appId": {
            "required": true,
            "type": "string"
        },
        "Version": {
            "required": true,
            "type": "string"
        },
        "requestBody": {
            "description": " Request structure for create Domain Association request. ",
            "properties": {
                "domainName": {
                    "description": " Domain name for the Domain Association. ",
                    "maxLength": 255,
                    "type": "string"
                },
                "enableAutoSubDomain": {
                    "description": " Enables automated creation of Subdomains for branches. ",
                    "type": "boolean"
                },
                "subDomainSettings": {
                    "description": " Setting structure for the Subdomain. ",
                    "items": {
                        "description": " Setting for the Subdomain. ",
                        "properties": {
                            "branchName": {
                                "description": " Branch name setting for the Subdomain. ",
                                "maxLength": 255,
                                "minLength": 1,
                                "type": "string"
                            },
                            "prefix": {
                                "description": " Prefix setting for the Subdomain. ",
                                "maxLength": 255,
                                "type": "string"
                            }
                        },
                        "required": [
                            "prefix",
                            "branchName"
                        ],
                        "type": "object"
                    },
                    "maxItems": 255,
                    "type": "array"
                }
            },
            "required": [
                "domainName",
                "subDomainSettings"
            ],
            "type": "object"
        },
        "X_Amz_Content_Sha256": {
            "required": false,
            "type": "string"
        },
        "X_Amz_Date": {
            "required": false,
            "type": "string"
        },
        "X_Amz_Algorithm": {
            "required": false,
            "type": "string"
        },
        "X_Amz_Credential": {
            "required": false,
            "type": "string"
        },
        "X_Amz_Security_Token": {
            "required": false,
            "type": "string"
        },
        "X_Amz_Signature": {
            "required": false,
            "type": "string"
        },
        "X_Amz_SignedHeaders": {
            "required": false,
            "type": "string"
        }
    }
}